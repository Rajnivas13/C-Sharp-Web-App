@model IEnumerable<learningsharp.Models.Category>


<div class="container">
    <div class="row pt-5">
        <div class="col-6">
            <h2>Category list</h2>
        </div>
        <div class="col-6">
            <h2>New Category</h2>
        </div>
    </div>
</div>

<br /><br />

@if (Model.Count() > 0) {
<div class="row pt-3">
    <table class="table table-bordered table-striped" style="width=100%;">
        <thead>
            <tr>
                <th>
                    Category Name
                </th>
                <th>
                    Display order
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="50%">@obj.Name</td>
                    <td width="50%">@obj.DisplayOrder</td>
                </tr>
            }
        </tbody>


    </table>
</div>
}
else { 
<div><h1>Sorry i can't retrive</h1></div>
}
    



